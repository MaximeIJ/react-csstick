{
  "name": "@maximeij/react-csstick",
  "version": "0.1.1",
  "type": "module",
  "description": "CSS Stick figures and comics",
  "author": "Maxime IJ",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/MaximeIJ/react-csstick/issues"
  },
  "homepage": "https://github.com/MaximeIJ/react-csstick#readme",
  "keywords": [
    "css",
    "css-art",
    "stick",
    "comic",
    "react"
  ],
  "main": "./lib/react-csstick.umd.js",
  "module": "./lib/react-csstick.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/react-csstick.js",
      "require": "./lib/react-csstick.umd.js"
    },
    "./css": {
      "import": "./lib/style.css",
      "require": "./lib/style.css"
    }
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "dependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.15",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-react": "^2.1.0",
    "clean-package": "^2.1.1",
    "eslint": "8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.6.5"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix && prettier -w .",
    "prepublishOnly": "clean-package"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MaximeIJ/react-csstick.git"
  },
  "clean-package": {
    "extends": "clean-package/common",
    "remove": [
      "clean-package",
      "dependencies",
      "devDependencies",
      "peerDependencies",
      "publishConfig",
      "scripts"
    ]
  }
}
